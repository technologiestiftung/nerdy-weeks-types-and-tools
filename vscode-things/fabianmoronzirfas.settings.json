      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    },
    {
      "name": "tomato",
      "value": "#ff6347"
    }
  ],
  "breadcrumbs.enabled": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabCloseButton": "left",
  "workbench.editor.highlightModifiedTabs": true,
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.location": "scm",
  "gitlens.views.compare.location": "scm",
  "gitlens.views.search.location": "scm",
  "terminal.explorerKind": "external",
  "merge-conflict.diffViewPosition": "Below",
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "asciidecorator.defaultFont": "Bloody",
  "workbench.iconTheme": "material-icon-theme",
  "diffEditor.ignoreTrimWhitespace": false,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "keyword",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "language",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "vue",
    "typescript",
    "javascript",
    "javascriptreact",
    "typescriptreact"
  ],
  "scm.defaultViewMode": "tree",
  "polacode.transparentBackground": true,
  "debug.toolBarLocation": "docked",
  "git.untrackedChanges": "separate",
  "task.slowProviderWarning": ["npm"],
  "yaml.schemas": {
    "file:///Users/icke/.vscode/extensions/me-dutour-mathieu.vscode-github-actions-2.2.3/schemas/action.schema.json": "/action.yml"
  },
  "javascript.format.semicolons": "insert",
  "workbench.colorTheme": "Nord",
  "codespaces.accountProvider": "Microsoft"
}
