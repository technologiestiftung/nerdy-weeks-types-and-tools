{
  "tabnine.experimentalAutoImports": true,
  "workbench.editor.limit.perEditorGroup": true,
  "editor.formatOnSave": true,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "editor.renameOnType": true,
  "liveshare.featureSet": "insiders",
  "liveshare.audio.joinCallBehavior": "accept",
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.limit.value": 5,
  "editor.formatOnType": true,
  "remote.extensionKind": {
    "ms-azuretools.vscode-docker": "workspace"
  },
  "git.confirmSync": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "[liquid]": {
    "editor.formatOnSave": false
  },
  "[vue]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "jekyll": "html",
    "scss": "css",
    "liquid": "html"
  },
  "terminal.integrated.letterSpacing": 1,
  "window.zoomLevel": 0,
  "editor.renderWhitespace": "all",
  "terminal.integrated.cursorBlinking": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontFamily": "Fira Mono for Powerline",
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "editor.fontFamily": "'Dank Mono', 'Victor Mono', monospace",
  "editor.scrollBeyondLastLine": false,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 80,
  /* Next one is very opintionated */
  "workbench.sideBar.location": "right",
  "css.validate": true,
  "scss.validate": true,
  "files.trimTrailingWhitespace": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "files.associations": {
    "*.fish": "fish",
    ".skhdrc": "shellscript",
    "*.conf": "NGINX",
    "*.html": "html"
  },

  "spellright.language": ["en", "de"],
  "spellright.documentTypes": ["markdown", "latex", "plaintext"],
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDelete": false,
  "editor.minimap.side": "right",
  /*This is for my personal usage and might not be for you*/
  "openinmarked.apppath": "/Applications/Setapp/Marked.app",
  "editor.fontLigatures": true,
  "editor.wrappingIndent": "indent",
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "javascript.suggest.completeFunctionCalls": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "licenser.author": "Technologiestiftung Berlin & Fabian Mor√≥n Zirfas",
  "licenser.license": "MIT",
  "licenser.disableAutoHeaderInsertion": true,
  "editor.tabCompletion": "on",
  "search.quickOpen.includeSymbols": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "git.autofetch": true,
  "togglequotes.chars": ["\"", "'", "`"],
  "typescript.preferences.quoteStyle": "double",
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "markdown-preview-enhanced.enableTypographer": true,
  "markdown-preview-enhanced.mermaidTheme": "default",
  "yaml.format.enable": true,
  "editor.detectIndentation": true,
  "editor.renderIndentGuides": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.list.keyboardNavigation": "filter",
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  },

  "peacock.affectActivityBar": true,
  "peacock.affectStatusBar": true,
  "peacock.surpriseMeOnStartup": true,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
